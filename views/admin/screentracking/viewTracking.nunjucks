                              <td>{{ lg }}</td>
                              <td>{{ logdata.logreference }}</td>
                              <td>{{ logdata.modulename }}</td>
                              <td>{{ logdata.logmessage }}</td>
                              <td data-sort='MMDDYYYY'>{{ logdata.createdAt }}</td>
                            </tr>
                          {% endfor %}

                            <th>Module Name</th>
                            <th>Message</th>
                            <th>Created Date</th>
                          </tr>
                        </thead>
                        <tbody>

                          {% set lg = 0 %}
                          {% for logdata in logDetails %}
                            {% set lg = lg + 1 %}

			<tr>
				<td>4.</td>
				<td>Down Payment</td>
				<td style="text-align: right;">$</td>
				<td style="text-align: right; border-bottom: solid 1px;">{{ downpayment | $format( false ) }}</td>
				<td>(4)</td>
			</tr>
			<tr>
				<td>5.</td>
				<td>Total&nbsp;Amount&nbsp;Financed<br/>(4&nbsp;less&nbsp;5)</td>

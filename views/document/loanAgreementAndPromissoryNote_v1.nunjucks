						<tr>
							<td colspan="2">TOTAL CASH PRICE AND TAXES</td>
							<td></td>
							<td></td>
						</tr>
					</table>
				</td>
				<td style="text-align: right; vertical-align: bottom; width: 28%" >$</td>
				<td style="text-align: right; border-bottom: solid 1px; vertical-align: bottom; width: 14%">{{ cashPrice | $format( false ) }}</td>
				<td style="vertical-align: bottom; width: 7%">(1)</td>
			</tr>
			<tr>
				<td>2.</td>
				<td>Amount Paid to Others:<span style="padding-left: 90px; padding-right: 5px;">$</span><span style="border-bottom: 1px solid; padding-right: 5px; padding-left: 45px;">0.00</span></td>
				<td style="text-align: right;">$</td>
				<td style="text-align: right; border-bottom: solid 1px;">0.00</td>
				<td></td>
			</tr>
			<tr>
				<td>3.</td>
				<td>Total (1 + 2)</td>
				<td style="text-align: right;">$</td>
				<td style="text-align: right; border-bottom: solid 1px;">{{ cashPrice | $format( false ) }}</td>
